suites:
  tagattributes:
    namespace: drupol\Anonymize
    psr4_prefix: drupol\Anonymize

formatter.name: pretty

extensions:
  LeanPHP\PhpSpec\CodeCoverage\CodeCoverageExtension:
    blacklist:
      - src/test
    format:
      - html
      - clover
      - php
      - text
    output:
      html: build/coverage
      clover: build/coverage.xml
      php: build/coverage.php